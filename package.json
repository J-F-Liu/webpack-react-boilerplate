    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "command-line-args": "^4.0.4",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^15.0.0",
