{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "webpack-react-boilerplate",
  "main": "webpack.config.js",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "cross-env NODE_ENV=production webpack",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "lint": "eslint --ext .js,.jsx app",
    "build:server": "cross-env NODE_ENV=production babel app -d build/server/app && babel server/entry.jsx -d build",
    "serve": "npm run build:server && node build/server/entry.js"
  },
  "keywords": [],
  "author": "Junfeng Liu",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "img-loader": "^1.2.2",
    "koa": "^2.0.0",
    "koa-static": "^3.0.0",
    "node-sass": "^3.6.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.12.0"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1",
    "sugar": "^1.4.1"
  }
}
