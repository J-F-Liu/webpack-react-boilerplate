{
  "name": "webpack-react-boilerplate",
  "version": "2.0.0",
  "description": "webpack-react-boilerplate",
  "main": "webpack.config.js",
  "scripts": {
    "prebuild": "rimraf dist/client",
    "build": "cross-env NODE_ENV=production webpack",
